class Solution {
public:
    int findMin(vector<int>& nums) {
        
        int n = nums.size();

         int i = -1;
         int j = n;

         while(j-i>1){
             int m = i+(j-i)/2;

             if(nums[m] <= nums[n-1])j=m;
             else i=m;
         }

         return nums[j];

    }
};
